<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.DataEvenementController">
   <children>
      <TableView fx:id="tableP" layoutX="137.0" layoutY="25.0" onSort="#LoadData" prefHeight="200.0" prefWidth="279.0">
        <columns>
          <TableColumn fx:id="TbId" prefWidth="75.0" text="Id" />
          <TableColumn fx:id="TbNom" prefWidth="94.0" text="Nom" />
            <TableColumn fx:id="TbPrenom" prefWidth="109.0" text="Prenom" />
        </columns>
      </TableView>
      <Button fx:id="btnUpdate" layoutX="183.0" layoutY="297.0" mnemonicParsing="false" onAction="#Update" text="Update" />
      <Button fx:id="btnDelete" layoutX="300.0" layoutY="297.0" mnemonicParsing="false" onAction="#DeleteParticipant" text="Delete" />
   </children>
</AnchorPane>
